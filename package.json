{
  "name": "docs.rainprotocol.xyz",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "pull-rain-protocol": "npm install beehive-innovation/rain-protocol.git#develop-sale",
    "pull-rain-sdk": "npm install rain-sdk",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build-rain-docs": "solidity-docgen -i ./node_modules/@beehiveinnovation/rain-protocol/contracts/ -o ./docs/developer-tools/smart-contracts/ --solc-module solc-0.8 --exclude ./node_modules/@beehiveinnovation/rain-protocol/contracts/test/ -t ./.templates/",
    "build-rain-sdk-docs": "rm -rf \"./docs/developer-tools/sdk/!(_category_.json)\" && cp -r ./node_modules/rain-sdk/docs/. ./docs/developer-tools/sdk",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "veamos": "nix-shell --run docs-version",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "BUILD-AND-DEPLOY": "npm run build && npm run-script deploy",
    "UPDATE-BUILD-AND-DEPLOY": "npm run pull-rain-protocol && npm run pull-rain-sdk && npm run build-rain-docs && build-rain-sdk-docs && npm run build && npm run-script deploy",
    "deploy": "aws --profile unegma s3 sync --delete ./build/ s3://docs.rainprotocol.xyz/",
    "deploy-autogenerated": "# todo: what does this do? docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.17",
    "@docusaurus/preset-classic": "2.0.0-beta.17",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@beehiveinnovation/rain-protocol": "github:beehive-innovation/rain-protocol#e590ce7ef673224afba04c43384a278a9500500d",
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/module-type-aliases": "2.0.0-beta.17",
    "@tsconfig/docusaurus": "^1.0.4",
    "hardhat": "^2.9.9",
    "prettier": "^2.6.0",
    "rain-sdk": "^1.2.10",
    "solc-0.8": "npm:solc@0.8.10",
    "solidity-docgen": "^0.5.16",
    "typescript": "^4.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
